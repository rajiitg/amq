web:    java -Djetty.port=$PORT -Dactivemq.base=target -Dactivemq.home=tools -Dactivemq.conf=tools/conf -Dactivemq.data=tools/data -Dactivemq.classpath=tools/conf -Djava.util.logging.config.file=logging.properties -Djava.security.auth.login.config=tools/conf/login.config $JAVA_OPTS -cp target/lib/*.jar -jar target/amq.jar start
